    <declare-styleable name="XSubMenuButton">
        <attr name="XSubMenuButtonText" format="string" />
    </declare-styleable>
    <declare-styleable name="XExpandableMenuButton">
        <attr name="XExpandableMenuButtonText" format="string" />
    </declare-styleable>
    <declare-styleable name="XMenuGroup">
        <attr name="XMenuGroupTitle" format="string" />
